rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    match /user/{name} {
  	  allow read,write: if request.auth != null;
		}

    match /user/{uemail} {
  		allow read,write: if request.auth != null;
		}
    
    match /libros/{nombre}{
    	allow read: if true;
    }

    match /libros/{precio}{
    	allow read: if true;
    }

    match /libros/{autor}{
    	allow read: if true;
    }
  }
}